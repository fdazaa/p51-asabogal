{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "google/charts",
                "version": "46",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "google_charts"
                },
                "dist": {
                    "url": "https://www.gstatic.com/charts/loader.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "chartjs/chartjs",
                "version": "v3.5.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chartjs"
                },
                "dist": {
                    "url": "https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.5.1/chart.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/highcharts",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/highcharts.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/more",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_more"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/highcharts-more.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/exporting",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_exporting"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/modules/exporting.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/export-data",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_export-data"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/modules/export-data.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/accessibility",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_accessibility"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/modules/accessibility.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/3d",
                "version": "9.2.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts_3d"
                },
                "dist": {
                    "url": "https://code.highcharts.com/9.2.2/highcharts-3d.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~2.0"
                }
            }
        }

    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advancedqueue": "^1.0@RC",
        "drupal/backup_migrate": "^5.0",
        "drupal/business_rules": "^2.0@beta",
        "drupal/charts": "^5.0@alpha",
        "drupal/commerce": "^2.24",
        "drupal/commerce_license": "^2.0@alpha",
        "drupal/commerce_recurring": "^1.0@beta",
        "drupal/core-composer-scaffold": "9.2",
        "drupal/core-project-message": "9.2",
        "drupal/core-recommended": "9.2",
        "drupal/cshs": "^3.3",
        "drupal/devel": "^4.1",
        "drupal/entity_browser": "2.6",
        "drupal/epayco": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/field_name_prefix_remove": "^1.0",
        "drupal/field_permissions": "^1.1",
        "drupal/file_browser": "^1.3",
        "drupal/fontawesome": "^2.19",
        "drupal/fontawesome_menu_icons": "^1.9",
        "drupal/formdazzle": "^2.1",
        "drupal/forms_steps": "^1.5",
        "drupal/gamification": "1.0",
        "drupal/ginvite": "^2.0",
        "drupal/group": "^1.4",
        "drupal/hide_revision_field": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/machine_name_widget": "^2.0@alpha",
        "drupal/menu_per_role": "^1.3",
        "drupal/quiz": "^6.0@alpha",
        "drupal/reference_table_formatter": "^2.0@alpha",
        "drupal/smtp": "^1.0",
        "drupal/subgroup": "1.0",
        "drush/drush": "^10.6",
        "highcharts/3d": "9.2.2",
        "highcharts/accessibility": "9.2.2",
        "highcharts/export-data": "9.2.2",
        "highcharts/exporting": "9.2.2",
        "highcharts/highcharts": "9.2.2",
        "highcharts/more": "9.2.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "patches": {
            "drupal/business_rules": {
                "Activa acciones en condiciones": "web/patch/businessrules/cannot_add_action-3200711-29.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
